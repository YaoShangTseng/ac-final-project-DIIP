<%#= link_to "新增個人檔案", new_profile_path, class: "btn btn-default" %>


<table class="table">
  <thead>
    <tr>
      <th>暱稱</th>
    </tr>
  </thead>
  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <% unless profile.is_current_user?(current_user) %>
          <td><%= link_to profile.nickname, profile_path(profile) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
